{
  "name": "angular-architecture",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "i18n": "ng xi18n --output-path src/locales",
    "dev:pt-br": "ng serve --aot --i18n-file=src/locales/messages.pt-BR.xlf --locale=pt-BR",
    "dev:en-us": "ng serve --aot --i18n-file=src/locales/messages.en-US.xlf --locale=en-US",
    "prod:pt-br": "ng serve --prod --aot --i18n-file=src/locales/messages.pt-BR.xlf --locale=pt-BR",
    "prod:en-us": "ng serve --prod --aot --i18n-file=src/locales/messages.en-US.xlf --locale=en-US",
    "build:pt-br": "ng build --output-path=dist/pt-br --aot -prod --bh /pt-br/ --i18n-file=src/locales/messages.pt-BR.xlf --locale=pt-BR",
    "build:en-us": "ng build --output-path=dist/en-us --aot -prod --bh /en-us/ --i18n-file=src/locales/messages.en-US.xlf --locale=en-US",
    "build": "npm run build:pt-br && npm run build:en-us",
    "deploy": "firebase deploy",
    "start": "ng serve"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.4.4",
    "@angular/cdk": "^2.0.0-beta.12",
    "@angular/common": "^4.4.4",
    "@angular/compiler": "^4.4.4",
    "@angular/core": "^4.4.4",
    "@angular/flex-layout": "^2.0.0-beta.9",
    "@angular/forms": "^4.4.4",
    "@angular/http": "^4.4.4",
    "@angular/material": "^2.0.0-beta.12",
    "@angular/platform-browser": "^4.4.4",
    "@angular/platform-browser-dynamic": "^4.4.4",
    "@angular/router": "^4.4.4",
    "core-js": "^2.5.1",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "mdi": "^2.0.46",
    "rxjs": "^5.4.3",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^1.4.5",
    "@angular/compiler-cli": "^4.4.4",
    "@types/jasmine": "2.6.0",
    "@types/node": "~8.0.34",
    "codelyzer": "~3.2.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-sass": "^4.9.0",
    "protractor": "~5.1.2",
    "ts-node": "~3.3.0",
    "tslint": "~5.7.0",
    "typescript": "^2.3.2"
  }
}
